#include <stdlib.h>
#include <stdio.h>
#include <unistd.h>
#include <sys/types.h>
#include <sys/stat.h>
#include <fcntl.h>


int main(){
  umask(0);
  int fd;
  struct stat sb;
  fstat(fd,&sb);
  fd = open("stuff", O_CREAT|O_RDWR, 0644);
  printf("fd: %d\n",fd);
  printf("protection: %d\n",sb.st_mode);
  return 0;
}
